{
  "name": "tractians-challenger",
  "version": "1.0.0",
  "description": "A challenger maid by Thiago",
  "title": "Tractian's challenger",
  "author": "ThiagoSpart",
  "repository": {
    "type": "git",
    "url": "https://github.com/Thiago-spart/tractian-challenger"
  },
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "commit": "git-cz",
    "test": "jest",
    "coverage": "yarn jest --coverage",
    "lint": "yarn lint:eslint && yarn lint:css",
    "lint:eslint": "eslint --ext js,jsx,ts,tsx .",
    "lint:css": "stylelint **/*.{ts,tsx}",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "tsc:client": "tsc --noEmit -p tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./dist_storybook",
    "analyze": "cross-env ANALYZE=true next build",
    "bump-version": "standard-version --skip.commit --skip.tag"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && yarn tsc",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,js,jsx}": [
      "stylelint",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@trendyol-js/react-carousel": "^3.0.0",
    "axios": "^1.2.2",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "i18next": "^22.4.8",
    "next": "^13.1.1",
    "next-i18next": "^13.0.3",
    "react": "^18.2.0",
    "react-click-away-listener": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.5",
    "react-i18next": "^12.1.1",
    "react-icons": "^4.7.1",
    "react-is": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-toastify": "^9.1.1",
    "styled-components": "^5.3.6",
    "swiper": "^8.4.5",
    "webpack": "^5.75.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@next/bundle-analyzer": "^13.1.1",
    "@techmmunity/eslint-config": "^5.2.4",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-modal": "^3.13.1",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-styled-components": "^2.0.7",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-specific-snapshot": "^7.0.0",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.4"
  }
}
